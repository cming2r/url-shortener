{
  "$schema": "node_modules/wrangler/config-schema.json",
  "name": "url-shortener-api",
  "main": "src/index.js",
  "compatibility_date": "2024-02-12",
  "kv_namespaces": [
    {
      "binding": "URL_STORE",
      "id": "您的KV命名空間ID"
    }
  ],
  "routes": [
    {
      "pattern": "vvrl.cc/*",
      "custom_domain": true
    }
  ],
  "observability": {
    "enabled": true
  }
}